<div class="container overall_container">
	<div class="col-sm-3">
		<h1>Hey <%= @user.username %>!</h1>
		<h3>all your bookmarks are loaded in panels</h3>
		

		<div class="btn-group">
  			<button class="btn btn-default btn-lg dropdown	-toggle" type="button" data-toggle="dropdown" 	aria-haspopup="true" aria-expanded="false">
    		Large button <span class="caret"></span>
  			</button>
  			<ul class="dropdown-menu">
    			<li>
					<%= link_to "add new bookmark", 	new_bookmark_path ,class: "btn btn	-default" %>
    			</li>
	
				<li>
    				<%= link_to "edit account info", 	edit_user_path ,class: "btn btn-	default" %>
    			</li>
	
    			<li>
    				<%= link_to "delete account", @current_user ,class: "btn btn-	default", :method => 'delete' %>
    			</li>
  			</ul>
		</div>
	</div>

	<div class="bookmark_cards col-sm-9">
	<% for @item in @all_url %>
		<%= render "layouts/card" %>
	<% end %>
</div>
</div>